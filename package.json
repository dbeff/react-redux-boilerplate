{
  "name": "reactus",
  "version": "0.1.0",
  "description": "React Redux Developer Kit",
  "repository": "git@github.com:reactus/reactus.git",
  "author": "Daniel Beff",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean:dev && npm run webpack:dev",
    "build": "npm run clean:prod && npm run webpack:prod ",
    "webpack:dev": "webpack-dev-server --config config/webpack.config.dev.js --hot",
    "webpack:prod": "webpack -p --config config/webpack.config.prod.js",
    "clean:dev": "rimraf .tmp",
    "clean:prod": "rimraf dist"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ]
  },
  "browserslist": [
    "last 5 version"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-reactus": "^0.1.1",
    "eslint-loader": "^1.6.1",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "frontendler-sass": "^0.3.0",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.3.0",
    "open-browser-webpack-plugin": "0.0.3",
    "postcss-load-config": "^1.1.0",
    "postcss-loader": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "apache-server-configs": "^2.14.0",
    "axios": "^0.15.3",
    "fastclick": "^1.0.6",
    "history": "^3.0.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "object-assign": "^4.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  }
}
